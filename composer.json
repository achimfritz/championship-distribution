{
   "name": "achim-fritz/champion-ship-distriubtion",
      "description" : "Achim Fritz ChampionShip Distribution",
      "license": "LGPL-3.0+",
      "repositories": [
         {
            "type": "composer",
            "url": "http://ci.typo3.robertlemke.net/job/composer-packages/ws/repository/"
         },
         {
            "type": "vcs",
            "url": "git@github.com:achimfritz/AchimFritz.ChampionShip.git"
         },
         {
            "type": "vcs",
            "url": "git@github.com:achimfritz/AchimFritz.ChampionShip.Import.git"
         },
         {
            "type": "vcs",
            "url": "git://git.typo3.org/Packages/TYPO3.Twitter.Bootstrap.git"
         },
         {
            "type": "vcs",
            "url": "https://github.com/svparijs/TYPO3.AccountManagement.git"
         }
         
      ],
      "config": {
