{
  "name": "achim-fritz/champion-ship-distriubtion",
  "description": "Achim Fritz ChampionShip Distribution",
  "license": "LGPL-3.0+",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:achimfritz/AchimFritz.ChampionShip.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:achimfritz/AchimFritz.ChampionShip.Import.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/svparijs/TYPO3.AccountManagement.git"
    }

  ],
  "config": {
    "vendor-dir": "Packages/Libraries",
    "bin-dir": "bin"
  },
  "minimum-stability": "dev",
  "require": {
    "typo3/flow": "3.0.*",
    "typo3/swiftmailer": "dev-master",
    "typo3/party": "~3.0",
    "achimfritz/championship": "dev-master",
    "achimfritz/championship-import": "dev-master",
    "typo3/accountmanagement": "dev-master"
  },
  "require-dev": {
    "typo3/kickstart": "3.0.*",
    "typo3/buildessentials": "3.0.*",
    "mikey179/vfsstream": "~1.6"
  },
  "scripts": {
    "post-update-cmd": "TYPO3\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
    "post-install-cmd": "TYPO3\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall"
  }
}
